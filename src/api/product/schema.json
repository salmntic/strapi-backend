{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "stock": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "is_preorder": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "seo_title": {
      "type": "string"
    },
    "seo_description": {
      "type": "text"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "products"
    }
  }
}