{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Products"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "description": {
      "type": "richtext"
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "discountPrice": {
      "type": "decimal"
    },
    "preorder": {
      "type": "boolean",
      "default": false
    },
    "stockStatus": {
      "type": "enumeration",
      "default": "inStock",
      "enum": [
        "inStock",
        "outOfStock",
        "limitedStock"
      ]
    },
    "seoTitle": {
      "type": "string"
    },
    "seoDescription": {
      "type": "text"
    },
    "seoKeywords": {
      "type": "string"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "customerReviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::customer-review.customer-review",
      "mappedBy": "product"
    },
    "orders": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::order.order",
      "inversedBy": "products"
    }
  }
}
